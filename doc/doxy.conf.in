PROJECT_NAME = "The ROme OpTimistic Simulator"
PROJECT_LOGO = "img/rootsim.png"
PROJECT_NUMBER = "0.9.0-RC1"
PROJECT_BRIEF = "An Open Source General Purpose Simulation Platform"
OUTPUT_LANGUAGE = "English"
OUTPUT_DIRECTORY = "." 
GENERATE_HTML = YES
SOURCE_BROWSER = YES
GENERATE_LATEX = NO 
RECURSIVE = YES 
MACRO_EXPANSION = YES
OPTIMIZE_OUTPUT_FOR_C = YES
